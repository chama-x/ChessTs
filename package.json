{
  "name": "reactchessts",
  "version": "1.0.0",
  "description": "React TypeScript chess development toolkit",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/data/datasets"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.es.js"
    },
    "./datasets/*": "./src/data/datasets/*"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "publish:package": "node scripts/publish.js",
    "release": "npm run build && npm run publish:package",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "chess.js": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "keywords": [
    "react",
    "chess",
    "typescript",
    "board",
    "game",
    "chess-board",
    "chess-game",
    "chess-engine",
    "chess-ui"
  ],
  "author": "Chamath Thiwanka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chama-x/ReactChessTs.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
